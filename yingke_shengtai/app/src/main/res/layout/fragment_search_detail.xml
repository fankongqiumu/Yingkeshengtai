<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:orientation="vertical">
    <com.yingke.shengtai.view.TitleView
        android:id="@+id/fragment_title"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        />
    <jp.co.recruit_lifestyle.android.widget.WaveSwipeRefreshLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/main_swipe"
        android:layout_below="@id/fragment_title"
        >
        <ScrollView
            android:id="@+id/fragment_listView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:fillViewport="false">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_marginLeft="5dp"
                >
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    >
                    <ImageView
                        android:id="@+id/item_image_left"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:src="@mipmap/tab_busiess_norma"
                        android:layout_marginLeft="15dp"/>
                    <TextView
                        android:id="@+id/item_text_progress"
                        android:layout_width="75dp"
                        android:layout_height="28dp"
                        android:singleLine="true"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:layout_alignParentRight="true"
                        android:paddingTop="2dp"
                        android:paddingBottom="2dp"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:textSize="14sp"
                        android:textColor="@color/white_color"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toLeftOf="@id/item_text_progress"
                        android:layout_toRightOf="@id/item_image_left"
                        android:orientation="vertical"
                        android:layout_centerVertical="true"
                        >
                        <TextView
                            android:id="@+id/item_text_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:singleLine="true"
                            android:layout_marginLeft="15dp"
                            android:textSize="@dimen/fifty_sp"
                            android:ellipsize="end"
                            android:layout_marginRight="15dp"
                            android:textColor="@color/text_big_color"/>
                        <TextView
                            android:id="@+id/item_text_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:singleLine="true"
                            android:ellipsize="end"
                            android:layout_marginLeft="15dp"
                            android:paddingTop="6dp"
                            android:layout_marginRight="15dp"
                            android:textSize="@dimen/fourty_sp"
                            android:textColor="@color/text_little_color"/>
                    </LinearLayout>


                </RelativeLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="@color/line_color"/>
                <LinearLayout
                    android:id="@+id/line1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone"
                    android:padding="15dp">
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:gravity="center_vertical"
                        android:textColor="@color/text_big_color"
                        android:textSize="15sp"
                        android:text="@string/star_degree"/>
                    <LinearLayout
                        android:id="@+id/star"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <ImageView
                            android:id="@+id/star1"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:background="@mipmap/star_normal"/>
                        <ImageView
                            android:id="@+id/star2"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_marginLeft="10dp"
                            android:background="@mipmap/star_normal"/>
                        <ImageView
                            android:id="@+id/star3"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_marginLeft="10dp"
                            android:background="@mipmap/star_normal"/>
                        <ImageView
                            android:id="@+id/star4"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_marginLeft="10dp"
                            android:background="@mipmap/star_normal"/>
                        <ImageView
                            android:id="@+id/star5"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_marginLeft="10dp"
                            android:background="@mipmap/star_normal"/>


                    </LinearLayout>
                </LinearLayout>
                <View
                    android:id="@+id/linesssss"
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:visibility="gone"
                    android:background="@color/line_color"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="15dp"
                    >
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_big_color"
                        android:textSize="15sp"
                        android:text="@string/business_number"/>
                    <TextView
                        android:id="@+id/business_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_little_color"
                        android:textSize="15sp"
                        android:singleLine="true"
                        android:ellipsize="end"
                        android:layout_marginLeft="10dp"
                        />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="@color/line_color"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="15dp">
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_big_color"
                        android:textSize="15sp"
                        android:text="@string/hetong_number"/>
                    <TextView
                        android:id="@+id/hetong_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_little_color"
                        android:textSize="15sp"
                        android:singleLine="true"
                        android:ellipsize="end"
                        android:layout_marginLeft="10dp"
                        />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="@color/line_color"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="15dp">
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_big_color"
                        android:textSize="15sp"
                        android:text="@string/source"/>
                    <TextView
                        android:id="@+id/source"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_little_color"
                        android:textSize="15sp"
                        android:singleLine="true"
                        android:ellipsize="end"
                        android:layout_marginLeft="10dp"
                        />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="@color/line_color"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="15dp"
                    >
                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_big_color"
                        android:textSize="15sp"
                        android:text="@string/write_time"/>
                    <TextView
                        android:id="@+id/write_time"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_little_color"
                        android:textSize="15sp"
                        android:singleLine="true"
                        android:ellipsize="end"
                        android:layout_marginLeft="10dp"
                        />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:background="@color/line_color"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="15dp"
                    >
                    <TextView
                        android:id="@+id/people"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_big_color"
                        android:textSize="15sp"
                        android:layout_centerVertical="true"
                        android:text="@string/business_people"/>
                    <ImageView
                        android:id="@+id/contact_people"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="15dp"
                        android:src="@mipmap/tab_chat_normal" />
                    <TextView
                        android:id="@+id/business_people"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/text_little_color"
                        android:textSize="15sp"
                        android:singleLine="true"
                        android:ellipsize="end"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@id/contact_people"
                        android:layout_toRightOf="@id/people"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp"
                        />
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </jp.co.recruit_lifestyle.android.widget.WaveSwipeRefreshLayout>
</LinearLayout>
