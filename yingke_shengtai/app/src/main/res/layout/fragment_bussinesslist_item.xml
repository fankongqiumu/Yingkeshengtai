<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="120dp"
    >
    <ImageView
        android:id="@+id/item_image_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:src="@mipmap/tab_busiess_norma"
        android:layout_marginLeft="15dp"/>
    <TextView
        android:id="@+id/item_text_progress"
        android:layout_width="75dp"
        android:layout_height="28dp"
        android:singleLine="true"
        android:ellipsize="end"
        android:gravity="center"
        android:background="@mipmap/dark_blue_bg"
        android:layout_alignParentRight="true"
        android:layout_marginTop="35dp"
        android:layout_marginRight="15dp"
        android:textSize="14sp"
        android:textColor="@color/white_color"/>
    <TextView
        android:id="@+id/number1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:ellipsize="end"
        android:text="sdssddsddsds"
        android:layout_toRightOf="@id/item_image_left"
        android:layout_marginLeft="15dp"
        android:paddingTop="10dp"
        android:layout_marginRight="15dp"
        android:textSize="@dimen/fourty_sp"
        android:textColor="@color/text_little_color"/>
    <TextView
        android:id="@+id/number2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:ellipsize="end"
        android:text="sdssddsddsds"
        android:layout_toRightOf="@id/number1"
        android:paddingTop="10dp"
        android:layout_marginRight="15dp"
        android:textSize="@dimen/fourty_sp"
        android:textColor="@color/text_little_color"/>
    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:layout_marginLeft="15dp"
        android:textSize="@dimen/sixteen"
        android:ellipsize="end"
        android:layout_marginTop="5dp"
        android:text="sdssddsddsds"
        android:layout_marginRight="15dp"
        android:layout_toRightOf="@id/item_image_left"
        android:layout_below="@id/number1"
        android:textColor="@color/text_big_color"/>
    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:layout_marginLeft="15dp"
        android:textSize="@dimen/fourty_ban_sp"
        android:ellipsize="end"
        android:layout_marginTop="5dp"
        android:text="sdssddsddsds"
        android:layout_marginRight="20dp"
        android:layout_toRightOf="@id/item_image_left"
        android:layout_below="@id/title"
        android:textColor="@color/text_big_color"/>
    <TextView
        android:id="@+id/time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:singleLine="true"
        android:ellipsize="end"
        android:text="sdssddsddsds"
        android:paddingBottom="10dp"
       android:layout_marginLeft="15dp"
        android:layout_below="@id/name"
        android:layout_marginTop="5dp"
        android:textSize="@dimen/fourty_ban_sp"
        android:layout_marginRight="20dp"
        android:layout_toRightOf="@id/item_image_left"
        android:textColor="@color/text_big_color"/>

</RelativeLayout>